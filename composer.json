{
    "name": "charticms/charti",
    "description": "Chart√≠ is a free, lightweight, simple and modern content management system, made for minimal and medium websites.",
    "type": "cms",
    "version": "1.0.0",
    "keywords": ["publishing platform", "charti", "charticms", "content management system"],
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "George Lemon",
            "email": "hi@georgelemon.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "symfony/console": "5.1.3",
        "symfony/var-dumper": "^5.1.1",
        "symfony/cache": "^5.2@dev",
        "symfony/http-foundation": "^5.2@dev",
        "symfony/finder": "^5.1",
        "symfony/yaml": "^5.1",
        "psr/container": "^1.0",
        "illuminate/database": "^8.0",
        "illuminate/pagination": "^8.0",
        "illuminate/translation": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/pipeline": "^8.0",
        "illuminate/bus": "^8.0",
        "illuminate/events": "^8.0",
        "opis/closure": "^3.5",
        "indigophp/hash-compat": "^1.1",
        "ramsey/uuid": "^4.0",
        "phpmailer/phpmailer": "dev-master",
        "ezyang/htmlpurifier": "dev-master",
        "vlucas/phpdotenv": "^5.1@dev",
        "verbalexpressions/php-verbal-expressions": "dev-master",
        "georgelemon/dessert": "dev-master",
        "georgelemon/wups": "dev-master",
        "illuminate/cache": "^8.0",
        "nesbot/carbon": "^2.0@dev",
        "nette/php-generator": "^3.5@dev",
        "symfony/config": "5.x-dev",
        "davedevelopment/phpmig": "1.7.x-dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Charti\\Core\\" : "vendor/charti/core/src",
            "Dashboard\\" : "vendor/charti/dashboard/src",
            "Frontend\\" : "vendor/charti/frontend/src",
            "Charti\\Pluggable\\": "vendor/charti/pluggable/src",
            "Charti\\Support\\" : "vendor/charti/support/src",
            "Charti\\Components\\": "vendor/charti/ui-components/src",
            "Charti\\Rest\\": "vendor/charti/rest/src",
            "Database\\Seeds\\": "database/seeds"
        },
        "files":[
            "vendor/charti/core/src/helper.php",
            "vendor/charti/support/src/helper.php"
        ]
    },
    "minimum-stability": "dev"
}
